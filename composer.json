{
    "name": "gevara.su/MsgAgregator",
    "description": "Incorporate message agragator based on Yii 2 Advanced Project Template",
    "keywords": ["sms", "message", "agregator"],
    "homepage": "http://www.gevara.su/",
    "type": "project",
    "license": "GNU GPL v2",
    "support": {
        "email": "maks.neverov@gmail.com"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "scripts": {
        "post-install-cmd": "php init"
    }
}
